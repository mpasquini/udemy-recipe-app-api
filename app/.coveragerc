[run]
branch = True
source = .

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
precision = 2
skip_covered = True
show_missing = True
sort = Cover
omit =
    manage.py
    */tests/*
    app/wsgi.py
    */apps.py

[html]
directory = /coverage

[xml]
output = /coverage/coverage.xml